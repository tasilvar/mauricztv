.wp-caption img[class*="wp-image-"]{border:0 none;height:auto;margin:0;max-width:100%;padding:0;width:auto;}
.wp-caption .wp-caption-text{font-size:0.688rem;line-height:1.063rem;margin:0;padding:0.625rem;}
